import rospy
from tmc_yolo2_ros.yolo2_tf_publisher import Yolo2TFPublisher


def main():
    rospy.init_node('yolo2_tf_publisher')
    Yolo2TFPublisher()
    rospy.spin()

if __name__ == '__main__':
    main()
